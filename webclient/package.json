{
  "name": "webclient",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && rm -r ../dist/app/* && mv dist/* ../dist/app",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "emitter-js": "^2.0.2",
    "highlight.js": "^9.18.1",
    "nodriza": "git+https://github.com/nodriza-io/nodriza.git",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-json-pretty": "^1.6.3",
    "vue-router": "^3.1.5",
    "vue-toast-notification": "^0.1.0",
    "vuetify": "^2.2.18",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.3",
    "@vue/cli-plugin-vuex": "^4.2.3",
    "@vue/cli-service": "^4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-empty-pattern": "off",
      "no-async-promise-executor": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
